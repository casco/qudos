---
import Dialog from '@components/Dialog.astro'
import ResourceDivider from '@components/ResourceDivider.astro'
import Recommendations from '@components/Recommendations.astro'
import BibItem from '@components/BibItem.astro'
---
<script>
        const dialogs = [
            'dialog-math',
            'dialog-cubit',
            'dialog-gates',
            'dialog-multiple-cubits',
            'dialog-key-concepts',
            'dialog-first-algorithm',
            'dialog-unstructured-search',
            'dialog-fourier-transform',
            'dialog-factorization',
        ]
        let opened=false;
        for (let dialog of dialogs) {
            document.querySelectorAll(`[id^="${dialog}"]`).forEach((g) => {
                g.addEventListener("click", () => {
                    opened=true;
                    const d = document.getElementById(dialog) as HTMLDialogElement;
                    if (!d) return;
                    d.showModal();
                    document.body.style.overflow = 'hidden';
                    d.addEventListener('click', function(event) {
                        var target = event.target as HTMLElement;
                        if (!target) return;
                        if (target.tagName === 'svg' || target.tagName === 'path' || target.tagName.toLowerCase() === 'button' && target.classList.contains('dialog-close')) {
                            d.close();
                            return;
                        }
                        var rect = d.getBoundingClientRect();
                        var isInDialog = (rect.top <= event.clientY && event.clientY <= rect.top + rect.height &&
                          rect.left <= event.clientX && event.clientX <= rect.left + rect.width);
                        if (!isInDialog) {
                            opened=false;
                            d.close();
                            document.body.style.overflow = 'auto';
                        }
                    });
                    d.addEventListener('close', function() {
                        opened=false;
                        document.body.style.overflow = 'auto';
                    });
                });
                g.addEventListener("mouseover", () => {
                    if (!opened) {
                        let gTyped = g as SVGElement;
                        gTyped.style.cursor = "pointer";
                    }
                });
            });
        }
</script>

